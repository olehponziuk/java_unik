+package org.example;
+
+public class Park extends Place implements Cloneable{
+    public int countAttractions;
+    public double area;
+    public double ticketPrice;
+    public Park(String namePlace, boolean hasHotels, boolean hasRestaurants, int ageLimit, String country, int countAttractions, double area, double ticketPrice) {
+        super(namePlace, hasHotels, hasRestaurants, ageLimit, country);
+        this.countAttractions = countAttractions;
+        this.area = area;
+        this.ticketPrice = ticketPrice;
+    }
+
+    @Override
+    protected Object clone() throws CloneNotSupportedException {
+        return super.clone();
+    }
+    @Override
+    public String toString()
+    {
+        return "Park: " + namePlace +
+                "\nArea: " + area +
+                "\nTicket price: " + ticketPrice +
+                "\nCountAttractions: " + countAttractions +
+                "\nAge Limit: " + ageLimit +
+                "\nCountry: " + country;
+    }
+
+    @Override
+    public int hashCode() {
+        return countAttractions * (namePlace.hashCode() + 52);
+    }
+
+    @Override
+    public boolean equals(Object obj) {
+        if (this == obj) {
+            return true;
+        }
+        if (obj == null || getClass() != obj.getClass()) {
+            return false;
+        }
+        Park park = (Park) obj;
+        return countAttractions == park.countAttractions &&
+                Double.compare(park.area, area) == 0 &&
+                Double.compare(park.ticketPrice, ticketPrice) == 0 &&
+                super.equals(obj);
+    }
+
+
+}
